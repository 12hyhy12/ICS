.pos 0x0
init:
	irmovl Stack, %esp
	irmovl Stack, %ebp
	call Main
	halt
	.align 4
ele1:
		.long 0x00a
ele2:
		.long 0x00c
ele3:
		.long 0x00b
Main:
	pushl %ebp
	rrmovl %esp,%ebp
	irmovl $3,%edx
	pushl %edx                        # count = 3
	irmovl ele1,%edx
	pushl %edx
	call bubble_a
	rrmovl %ebp,%esp
	popl %ebp
	ret



.pos 0x100
Stack:
